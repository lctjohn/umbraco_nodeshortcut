{
  "name": "OpenContent",
  "description": "Opens an Umbraco content node by its ID. Use it from the Umbraco backoffice content page.",
  "version": "1.1",
  "permissions": [
    "tabs", "http://*/umbraco", "https://*/umbraco"
  ],
  "icons": {
      "48": "opencontent_128.png",
      "128": "opencontent_128.png"
  },
  "browser_action": {
      "default_title": "Open Content",
      "default_icon": "opencontent_128.png",
	  "default_popup": "popup.html"
  },
  "manifest_version": 2
}
